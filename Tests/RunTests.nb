Cell[BoxData[{
 RowBox[{
  RowBox[{"printTestResults", "//", "ClearAll"}], "\n"}], "\n", 
 RowBox[{
  RowBox[{"printTestResults", "[", "tr_TestReportObject", "]"}], ":=", 
  RowBox[{"Module", "[", "\n", "\t", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"list", ",", "indx", ",", "time", ",", "noTests"}], "}"}], ",", 
    "\n", "\t", "\n", "\t", 
    RowBox[{
     RowBox[{"list", "=", 
      RowBox[{"getTestResults", "[", "tr", "]"}]}], ";", "\n", "\t", 
     RowBox[{"indx", "=", 
      RowBox[{"MapIndexed", "[", "\n", "\t\t", 
       RowBox[{
        RowBox[{
         RowBox[{"If", "[", "\n", "\t\t\t", 
          RowBox[{
           RowBox[{"MemberQ", "[", 
            RowBox[{"#1", ",", 
             RowBox[{
             "\"\<Failure\>\"", "|", "\"\<MessagesFailure\>\"", "|", 
              "\"\<Error\>\""}]}], "]"}], ",", "\n", "\t\t\t", 
           RowBox[{"First", "[", "#2", "]"}], ",", "\n", "\t\t\t", 
           "Nothing"}], "\n", "\t\t", "]"}], "&"}], ",", "\n", "\t\t", 
        "list"}], "\n", "\t", "]"}]}], ";", "\n", "\t", 
     RowBox[{"time", "=", 
      RowBox[{"Round", "[", 
       RowBox[{
        RowBox[{"QuantityMagnitude", "[", 
         RowBox[{"tr", "[", "\"\<TimeElapsed\>\"", "]"}], "]"}], ",", 
        "0.01"}], "]"}]}], ";", "\n", "\t", 
     RowBox[{"noTests", "=", 
      RowBox[{"Length", "[", 
       RowBox[{"tr", "[", "\"\<TestResults\>\"", "]"}], "]"}]}], ";", "\n", 
     "\t", "\n", "\t", 
     RowBox[{"Print", "[", 
      RowBox[{
      "noTests", ",", "\"\< tests run in \>\"", ",", "time", ",", 
       "\"\< seconds.\>\""}], "]"}], ";", "\n", "\t", "\n", "\t", 
     RowBox[{"If", "[", "\n", "\t\t", 
      RowBox[{
       RowBox[{"TrueQ", "@", 
        RowBox[{"tr", "[", "\"\<AllTestsSucceeded\>\"", "]"}]}], ",", "\n", 
       "\t\t", 
       RowBox[{"Print", "[", "\"\<All tests succeeded!\>\"", "]"}], ",", "\n",
        "\t\t", 
       RowBox[{
        RowBox[{"Print", "[", 
         RowBox[{
          RowBox[{"tr", "[", "\"\<TestsFailedCount\>\"", "]"}], ",", 
          "\"\< tests failed!\>\""}], "]"}], ";"}]}], "\n", "\t", "]"}], ";", 
     "\n", "\t", "\[IndentingNewLine]", "\t", 
     RowBox[{"Print", "@", 
      RowBox[{"Grid", "[", 
       RowBox[{"list", ",", "\n", "\t\t", 
        RowBox[{"Alignment", "\[Rule]", "Left"}], ",", "\n", "\t\t", 
        RowBox[{"Dividers", "\[Rule]", 
         RowBox[{"{", 
          RowBox[{"None", ",", 
           RowBox[{"{", 
            RowBox[{"2", "\[Rule]", "True"}], "}"}]}], "}"}]}], ",", "\n", 
        "\t\t", 
        RowBox[{"Background", "\[Rule]", 
         RowBox[{"{", 
          RowBox[{"None", ",", 
           RowBox[{"Thread", "[", 
            RowBox[{"indx", "\[Rule]", "Pink"}], "]"}]}], "}"}]}]}], "\n", 
       "\t", "]"}]}], ";", "\n", "\t", "tr"}]}], "\[IndentingNewLine]", 
   "]"}]}]}], "Code",
 CellLabel->"In[7]:=",ExpressionUUID->"7e06ad6f-eade-47b7-9992-593d8a7fb9ec"]
